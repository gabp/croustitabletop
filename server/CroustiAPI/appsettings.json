{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0"
      },
      "Https": {
        "Url": "https://0.0.0.0"
      }
    },
    "Certificates": {
      "Default": {
          "Path": "certs/crousti-cert.pfx"
      }
    }
  },
  "CroustiTabletop": {
    "ClientUrl": "https://gabp.github.io/croustitabletop",
    "Tokens": {
      "Issuer" : "CroustiServer",
      "Audience": "CroustiClient",
      "Secret": "",
      "ExpirationInHours": 24
    },
    "DuckDns": {
      "ServerIp": "",
      "DomainName": "",
      "Token": ""
    },
    "LetsEncrypt": {
      "AccountEmail": ""
    }
  }
}
